#
# Tilera network device configuration
#

config TILE_NET
	tristate "Tilera GBE/XGBE network driver support"
	depends on TILE
	default y
	select CRC32
	select TILE_GXIO_MPIPE if TILEGX
	select HIGH_RES_TIMERS if TILEGX
